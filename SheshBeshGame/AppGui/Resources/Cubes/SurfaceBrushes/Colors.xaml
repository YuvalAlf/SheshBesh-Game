<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <GeometryDrawing x:Key="BackgroundRectGeometry" Brush="#FFA06B3E">
        <GeometryDrawing.Pen>
            <Pen Brush="#FF301A04" Thickness="0.01"/>
        </GeometryDrawing.Pen>
        <GeometryDrawing.Geometry>
            <RectangleGeometry Rect="0,0 1,1"/>
        </GeometryDrawing.Geometry>
    </GeometryDrawing>
    
    <GeometryDrawing x:Key="CubesNumberCircleGeometry" Brush="#FFE0D8B3">
        <GeometryDrawing.Geometry>
            <EllipseGeometry RadiusX="0.1" RadiusY="0.1"/>
        </GeometryDrawing.Geometry>
    </GeometryDrawing>
</ResourceDictionary>