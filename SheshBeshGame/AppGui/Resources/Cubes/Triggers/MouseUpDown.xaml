<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Storyboard x:Key="RotateCube1Storyboard" DesiredFrameRate="30" TargetName="CubesViewport" TargetProperty="Children[1].(ModelVisual3D.Transform).(RotateTransform3D.Rotation).(AxisAngleRotation3D.Axis)">
        <StaticResource ResourceKey="AxisVectorRotationAnimation1"/>
    </Storyboard>

    <Storyboard x:Key="RotateCube2Storyboard" DesiredFrameRate="30" TargetName="CubesViewport" TargetProperty="Children[1].(ModelVisual3D.Transform).(RotateTransform3D.Rotation).(AxisAngleRotation3D.Angle)">
        <StaticResource ResourceKey="RotationAnimation"/>
    </Storyboard>

    <Storyboard x:Key="RotateCube1AxisStoryboard" DesiredFrameRate="30" TargetName="CubesViewport" TargetProperty="Children[2].(ModelVisual3D.Transform).(Transform3DGroup.Children).[0].(RotateTransform3D.Rotation).(AxisAngleRotation3D.Axis)">
        <StaticResource ResourceKey="AxisVectorRotationAnimation2"/>
    </Storyboard>

    <Storyboard x:Key="RotateCube2AxisStoryboard" DesiredFrameRate="30" TargetName="CubesViewport" TargetProperty="Children[2].(ModelVisual3D.Transform).(Transform3DGroup.Children).[0].(RotateTransform3D.Rotation).(AxisAngleRotation3D.Angle)">
        <StaticResource ResourceKey="RotationAnimation"/>
    </Storyboard>
</ResourceDictionary>